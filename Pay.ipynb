    "inp = input('Enter Hours: ')\n",
    "hours = float(inp)\n",
    "inp = input('Enter Rate: ')\n",
    "rate = float(inp)\n",
    "if hours > 40:\n",
    "    pay = hours * rate + (hours - 40) * rate * 0.5\n",
    "else:\n",
    "    pay = hours * rate\n",
    "print('Pay:', pay)\n"
